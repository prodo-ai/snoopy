{
  "name": "@prodo/snoopy-vscode-plugin",
  "displayName": "Snoopy VSCode Plugin",
  "description": "Fast feedback for React components.",
  "version": "0.0.1",
  "publisher": "prodo-ai",
  "categories": [
    "Other"
  ],
  "engines": {
    "vscode": "^1.28.0"
  },
  "activationEvents": [
    "onCommand:snoopy.extension.test",
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescript",
    "onLanguage:typescriptreact"
  ],
  "main": "./build/extension",
  "contributes": {
    "commands": [
      {
        "command": "snoopy.extension.test",
        "title": "Snoopy: Test"
      }
    ],
    "keybindings": [
      {
        "command": "snoopy.extension.test",
        "key": "shift+ctrl+v",
        "mac": "shift+cmd+v",
        "when": "editorLangId =~ /^javascript|^typescript"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ../../node_modules/vscode/bin/install",
    "test": "npm run compile && node ../../node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/node": "^8.10.25",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.21"
  },
  "dependencies": {}
}

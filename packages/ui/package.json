{
  "name": "@prodo-ai/snoopy-ui",
  "version": "0.1.0",
  "main": "public/index.html",
  "license": "MIT",
  "files": [
    "public",
    "src"
  ],
  "scripts": {
    "start": "parcel public/index.html",
    "build": "tsc --build",
    "clean": "rm -rf dist",
    "test": "PRODO_COMPONENTS_FILE=@prodo-ai/components jest",
    "lint": "set -ex; tsc --build; tslint --project ."
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@prodo-ai/snoopy-api": "0.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-responsive": "^6.1.1",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-select": "^2.4.3",
    "src": "^1.1.2",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.4",
    "@types/react-responsive": "^3.0.3",
    "@types/react-select": "^2.0.17",
    "@types/styled-components": "4.1.8",
    "jest": "^24.7.1",
    "jest-dom": "^3.1.3",
    "parcel-bundler": "^1.12.3",
    "react-testing-library": "^6.1.2",
    "ts-jest": "^24.0.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/build/"
    ],
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg|png)$": "<rootDir>/__mocks__/fileMock.js",
      "@prodo-ai/components$": "<rootDir>/__mocks__/componentsMock.ts"
    }
  },
  "alias": {
    "styled-components": "../../node_modules/styled-components"
  }
}

{
  "defaultSeverity": "error",
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "rules": {
    "interface-name": {
      "severity": "off"
    },
    "object-literal-sort-keys": {
      "severity": "off"
    },
    "no-implicit-dependencies": {
      "options": ["dev", ["~"]]
    },
    "no-submodule-imports": {
      "options": ["~"]
    }
  }
}
